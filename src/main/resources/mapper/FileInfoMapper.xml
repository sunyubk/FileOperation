<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sy.fileoperation.mapper.FileInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sy.fileoperation.entity.FileInfo">
        <id column="id" property="id" />
        <result column="file_name" property="fileName" />
        <result column="suff" property="suff" />
        <result column="location" property="location" />
        <result column="md5" property="md5" />
        <result column="status" property="status" />
        <result column="update_time" property="updateTime" />
        <result column="create_time" property="createTime" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_name, suff, location, md5, status, update_time, create_time
    </sql>

</mapper>
